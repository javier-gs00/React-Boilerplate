{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/proposal-class-properties",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "Assets": "./src/assets",
        "Components": "./src/components",
        "Containers": "./src/containers"
      }
    }]
  ]
}